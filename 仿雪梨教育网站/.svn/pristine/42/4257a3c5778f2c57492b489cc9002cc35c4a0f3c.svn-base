*{
	margin: 0;
	padding: 0;
	border:0;
}
body{
	font-family: "微软雅黑";
}
ul li{
	list-style-type: none;
	float: left;
}
/*daohang*/
.hover{
	background-color: #e7e7e7;
	border-bottom:2px solid orange;
}
.hover1{
	background-color: #e7e7e7;
}
.dh,.dh a{
	height: 48px;
}
.navbar a{
	color: #333;
}
.navbar a:hover{
	color: orange;
}
/*content*/
.bread{
	padding-top: 65px;
	padding-left: 10px;
	padding-bottom: 20px;
}
.home{
	color: rgb(255,110,61);
}
.divider{
	padding-left: 10px;
	padding-right: 10px;
	color: rgb(227,221,227);
	font-weight: bolder;
}
.title{
	color: rgb(227,221,227);
}
/*left*/
.left{
	margin-top: 40px;
}
.avatar{
	padding-top: 10px;
	margin-bottom: 5px;
}
.touxiang{
	width: 50px;
	height: 50px;
	border:1px solid #e5e5e5;
	border-radius: 3px;
	cursor: pointer;
	margin-left: 60px;
}
.name{
	font-size: 15px;
	padding-top: 7px;
	margin-left: 65px;
}
.first{
	padding-bottom: 5px;
}
.menu{
	font-size: 15px;
	color: black;
}
.menu:hover{
	color: rgb(255,155,61);
	text-decoration: none;
}
/*center*/
.center{

}
.fabu{
	padding: 2px 8px;
	border-radius: 4px;
	background-color: rgb(255,110,61);
	color: white;
}
.at-popup{
	width: 40px;
	height: 40px;
	border: 1px solid #e5e5e5;
	border-radius: 30px;
	margin-top: 20px;
}
.gossip-body{
	height: auto;
	padding-top: 10px;
	padding-right: 0px;
	padding-bottom: 5px;
	padding-left: 10px;
	clear right;
	margin-bottom:0;
}
.gossip-title a{
	width: 39px;
	height: 16px;
	color: #333;
	font-weight: bolder;
	font-size: 15px;
	text-decoration: none;
	transition-property: all;
	transition-duration: 0.5s;
	transition-timing-function: cubic-bezier(0.175, 0.885, 0.32, 1);
	transition-delay: initial;
}
.gossip-gossip a{
	color: rgb(255,110,61);
	text-decoration: none;
}
.btn{
	border:1px solid rgb(204,204,204);
	border-radius: 4px;
	background-color: rgb(242,242,242);
}
.btn:hover{
	border-radius: 4px;
	border:1px solid rgb(161,161,161);
	background-color: rgb(222,222,222);
}
.pull-left{
	margin-top: 10px;
}
.gossip-action{
	margin-top: 10px;
	color: rgb(170,170,197);
}
.gossip-action a{
	color: rgb(170,170,197);
}
.clearfix{
	padding-bottom: 15px;
	border-bottom: 1px solid #e5e5e5;
}
.shoucang,.forward,.reply,.like{
	text-decoration: none;
}
.shoucang:hover,.forward:hover,.reply:hover,.like:hover{
	color: rgb(255,110,61);
	text-decoration: none;
}
.zhangkai1{
	display:block;
	word-wrap:break-word;
	word-break:break-all;
	line-height:23px;
	margin-bottom:10px;
}
.shouqi1{
	display:none;
	word-wrap:break-word;
	word-break:break-all;
	line-height:23px;
	margin-bottom:10px;
}
.shouqi1 a{
    color: rgb(255,110,61);
	text-decoration: none;
}
.adjust-ajax{
	cursor: pointer;
}
/*right*/
.right{

}
.team{
	border:1px solid #e1e1e1;
	min-height: 140px;
	background-color: #fff;
	padding: 15px 20px;
	margin-bottom: 15px;
	border-radius: 3px;
}
.notask_text{
	display: none;
}
.task{
	text-align: center;
	padding-left: 10px;
	padding-top: 20px;
	margin-bottom: -80px;
	z-index: 200;
	float: left;
	display: inline-block;
}
canvas{
	border-radius: 50%;
	width: 75px;
	height: 75px;
	border: 5px solid #ccc;
	display: inline-block;
}
.zx{
	padding-top: 20px;
	min-height: 50px;
}
.goto{
	padding-top: 10px;
	padding-right: 10px;
	padding-bottom: 0;
	color: black;
	float: right;
}
.goto:hover{
	text-decoration: none;
	color: rgb(255,110,61);
}
/*footer*/
.address{
	text-align: center;
}
.copyright{
	text-align: center;
}
#hy{
	width: 300px;
	height: 50px;
	color: white;
	font-size: 14px;
	border:1px solid red;
	background-color: red;
	border-radius: 20px;
}